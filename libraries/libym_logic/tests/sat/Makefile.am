## Process this file with automake to produce Makefile.in


############################################################
## サブディレクトリの指定
############################################################

SUBDIRS =


############################################################
## clean ターゲットの指定
############################################################

CLEANFILES =

MAINTAINERCLEANFILES = Makefile.in


############################################################
## dist ターゲットの指定
############################################################

EXTRA_DIST =


############################################################
## build ターゲットの指定
############################################################

noinst_PROGRAMS = \
	dimacstest

bin_PROGRAMS = \
	ymsat \
	ymsat2


############################################################
## 汎用のプリプロセッサ用フラグの指定
############################################################

AM_CPPFLAGS = \
	-I$(top_srcdir)/include \
	-I$(top_srcdir)/../libym_utils/include \
	-I$(top_builddir)/../libym_utils/include


############################################################
## dimacstest 用の変数の指定
############################################################

dimacstest_SOURCES = \
	dimacstest.cc

dimacstest_LDADD = \
	$(top_builddir)/src/libym_logic.la

dimacstest_LDFLAGS =


############################################################
## ymsat 用の変数の指定
############################################################

ymsat_SOURCES = \
	ymsat.cc

ymsat_LDADD = \
	$(top_builddir)/src/libym_logic.la

ymsat_LDFLAGS =


############################################################
## ymsat2 用の変数の指定
############################################################

ymsat2_SOURCES = \
	ymsat2.cc

ymsat2_LDADD = \
	$(top_builddir)/src/libym_logic.la

ymsat2_LDFLAGS =
