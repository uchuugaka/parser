## Process this file with automake to produce Makefile.in


############################################################
## サブディレクトリの指定
############################################################

SUBDIRS =


############################################################
## clean ターゲットの指定
############################################################

CLEANFILES =

MAINTAINERCLEANFILES = Makefile.in


############################################################
## dist ターゲットの指定
############################################################

EXTRA_DIST =


############################################################
## build ターゲットの指定
############################################################

noinst_PROGRAMS = \
	dimacstest

bin_PROGRAMS = \
	ymsat \
	ymsat2


############################################################
## 汎用のプリプロセッサ用フラグの指定
############################################################

AM_CPPFLAGS = \
	-I$(YMTOOLS_SRCDIR)/include \
	-I$(YMTOOLS_BUILDDIR)/include


############################################################
## 共通の変数の定義
############################################################

LIBYM_SAT = \
	$(YMTOOLS_BUILDDIR)/libraries/libym_logic/libym_logic.la \
	$(YMTOOLS_BUILDDIR)/libraries/libym_utils/libym_utils.la


############################################################
## dimacstest 用の変数の指定
############################################################

dimacstest_SOURCES = \
	dimacstest.cc

dimacstest_LDADD = \
	$(LIBYM_SAT)

dimacstest_LDFLAGS =


############################################################
## ymsat 用の変数の指定
############################################################

ymsat_SOURCES = \
	ymsat.cc

ymsat_LDADD = \
	$(LIBYM_SAT)

ymsat_LDFLAGS =


############################################################
## ymsat2 用の変数の指定
############################################################

ymsat2_SOURCES = \
	ymsat2.cc

ymsat2_LDADD = \
	$(LIBYM_SAT)

ymsat2_LDFLAGS =
