## Process this file with automake to produce Makefile.in


############################################################
## サブディレクトリの指定
############################################################

SUBDIRS =


############################################################
## clean ターゲットの指定
############################################################

CLEANFILES =

MAINTAINERCLEANFILES = Makefile.in


############################################################
## dist ターゲットの指定
############################################################

EXTRA_DIST =


############################################################
## build ターゲットの指定
############################################################

bin_PROGRAMS = \
	read_blif \
	mindepth_test


############################################################
## 汎用のプリプロセッサ用フラグの指定
############################################################

AM_CPPFLAGS = \
	-I$(YMTOOLS_SRCDIR)/include \
	-I$(YMTOOLS_BUILDDIR)/include


############################################################
## 共通の変数の定義
############################################################

LIBYM_SBJ = $(YMTOOLS_BUILDDIR)/libraries/libym_sbj/libym_sbj.la
LIBYM_BNET = $(YMTOOLS_BUILDDIR)/libraries/libym_bnet/libym_bnet.la


############################################################
## read_blif 用の変数の定義
############################################################

read_blif_SOURCES = \
	read_blif.cc

read_blif_LDADD = \
	$(LIBYM_SBJ) \
	$(LIBYM_BNET)

read_blif_LDFLAGS =


############################################################
## mindepth_test 用の変数の定義
############################################################

mindepth_test_SOURCES = \
	mindepth_test.cc

mindepth_test_LDADD = \
	$(LIBYM_SBJ) \
	$(LIBYM_BNET)

mindepth_test_LDFLAGS =
